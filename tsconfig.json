{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2022",
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false
  },
  "exclude": [
    "node_modules"
  ]
}
